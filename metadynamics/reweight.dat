#RESTART
metad: READ FILE=../metad_data.dat VALUES=metad.rbias IGNORE_FORCES IGNORE_TIME
d2: READ FILE=../metad_data.dat VALUES=d2 IGNORE_FORCES IGNORE_TIME
d1: READ FILE=../metad_data.dat VALUES=d1 IGNORE_FORCES IGNORE_TIME


weights: REWEIGHT_METAD TEMP=298. ARG=metad.rbias
#weights2: REWEIGHT_BIAS TEMP=310.15 ARG=metad.rbias,bwall.bias,twall.bias




HISTOGRAM ...
  ARG=d2,d1
  GRID_MIN=0.2,0.2
  GRID_MAX=2.4,1.4
  GRID_BIN=150,150
  #BANDWIDTH=0.05
  KERNEL=DISCRETE
  LOGWEIGHTS=weights
  LABEL=hD
  UPDATE_FROM=100000
... HISTOGRAM


ff: CONVERT_TO_FES GRID=hD TEMP=298.
DUMPGRID GRID=ff FILE=rew_fes.dat 
